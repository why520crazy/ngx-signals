{
  "name": "ngx-signals",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build:docs": "docgeni build --prod",
    "start": "npm run start:docs",
    "build": "ng build",
    "start:docs": "docgeni serve --port 4666",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "pretty": "pretty-quick",
    "release": "npm run npm run build && wpm release",
    "release-only": "wpm release",
    "pub": "wpm publish && npm run pub-only",
    "pub-only": "cd dist/ngx-signals && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0-next.2",
    "@angular/common": "^16.0.0-next.2",
    "@angular/compiler": "^16.0.0-next.2",
    "@angular/core": "^16.0.0-next.2",
    "@angular/forms": "^16.0.0-next.2",
    "@angular/platform-browser": "^16.0.0-next.2",
    "@angular/platform-browser-dynamic": "^16.0.0-next.2",
    "@angular/router": "^16.0.0-next.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0-next.3",
    "@angular/cli": "~16.0.0-next.3",
    "@angular/compiler-cli": "^16.0.0-next.2",
    "@docgeni/cli": "^2.1.0-next.3",
    "@docgeni/template": "2.1.0-next.3",
    "@types/jasmine": "~4.3.0",
    "@worktile/pkg-manager": "^0.0.11",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.0.0-next.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.9.4"
  }
}
